{
  "cSpell.words": [
    "Elementor",
    "flowbite",
    "Googlecloud",
    "Hubspot",
    "jwiwac",
    "Microsoftazure",
    "Microsoftteams",
    "nextjs",
    "postbuild",
    "roadmap",
    "tailwindcss",
    "tryghost",
    "webappanalyzer",
    "webp"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
